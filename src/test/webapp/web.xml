<?xml version="1.0" encoding="UTF-8"?>
<web-app
        version="3.0"
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

    <context-param>
        <param-name>appPath</param-name>
        <param-value>com.thoughtworks.maomao.example</param-value>
    </context-param>

    <context-param>
        <param-name>templateName</param-name>
        <param-value>ST</param-value>
    </context-param>

    <listener>
        <listener-class>com.thoughtworks.maomao.core.NoamServletContextListener</listener-class>
    </listener>


</web-app>